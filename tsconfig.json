{
    "compilerOptions": {
        "outDir": "./src/scripts/js",
        "allowJs": true,
        "target": "es2017",
        "module": "commonjs",
        "allowSyntheticDefaultImports": true,
        "experimentalDecorators": true,
        "jsx": "react"
    },
    "include": [
        "./src/**/*.ts"
, "src/scripts/utils/ReactWrapper.tsx", "src/scripts/Core.ts"    ],
    "exclude": [
        "node_modules",
        "./src/*deprecated*",
    ],
    "awesomeTypescriptLoaderOptions": {
        "useCache": true
    }
}
